{
    "name": "kfilippovk/ozon",
    "description": "Implementation of API methods Ozon for Laravel.",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Filippov K.",
            "email": "filippovk997@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": ">=6.2.0",
        "illuminate/support": "^5.2|^6|^7|^8|^9",
        "illuminate/filesystem": "^5.2|^6|^7|^8|^9",
        "illuminate/cache": "^5.2|^6|^7|^8|^9",
        "ext-json": "*",
        "league/csv": "^9.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "sebastian/version": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
        "KFilippovk\\Ozon\\": "src/"
    }
    },
    "extra": {
        "laravel":{
            "providers": [
                "KFilippovk\\Ozon\\OzonServiceProvider",
                "KFilippovk\\Ozon\\OzonPerformanceServiceProvider"
            ],
            "aliases": {
                "Ozon": "KFilippovk\\Ozon\\Facades\\Ozon",
                "OzonPerformance": "KFilippovk\\Ozon\\Facades\\OzonPerformance"
            }
        }
    }
}
